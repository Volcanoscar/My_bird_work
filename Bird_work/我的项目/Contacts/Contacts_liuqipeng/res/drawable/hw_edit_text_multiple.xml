<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="true" android:state_window_focused="false">
        <nine-patch android:src="@drawable/hw_textfield_default2" />
    </item>
    <item android:state_enabled="false" android:state_window_focused="false">
        <nine-patch android:src="@drawable/hw_textfield_disabled2" />
    </item>
    <item android:state_enabled="true" android:state_focused="true">
        <nine-patch android:src="@drawable/hw_textfield_pressed" android:tint="@color/contact_editor_listview_divider_selected" />
    </item>
    <item android:state_enabled="true">
        <nine-patch android:src="@drawable/hw_textfield_default2" />
    </item>
    <item android:state_focused="true">
        <nine-patch android:src="@drawable/hw_textfield_default2" android:tint="@color/contact_editor_listview_divider_selected"/>
    </item>
    <item>
        <nine-patch android:src="@drawable/hw_textfield_disabled2" />
    </item>

</selector>
